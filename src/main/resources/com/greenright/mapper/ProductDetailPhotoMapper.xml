  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.greenright.dao.ProductDetailPhotoDao">
  <insert id="insert" parameterType="ProductDetailPhoto">
    insert into product_detail_photos(product_id,product_detail_photopath)
    values(#{productNo},#{photoPath})

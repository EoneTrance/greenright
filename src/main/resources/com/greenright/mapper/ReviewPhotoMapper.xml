  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.greenright.dao.ReviewPhotoDao">

 
  <insert id="insertReviewPhoto" parameterType="ReviewPhoto">
    insert into review_photos(review_id, photo_path)
    values(#{reviewNo},#{photoPath})
  </insert>
  
  
</mapper>




